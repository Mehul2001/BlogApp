<%include ./partials/header.ejs%>
    <% blogs.forEach(function(blog){%>
        <div class="container main">
            <h3>
                <%= blog.title%>
            </h3>
            <img src="<%=blog.image%>">
            <span><%=blog.created %></span>
            <p>
                <%=blog.body.substring(0,100)%>...
            </p>
            <a href="/blogs/<%=blog._id%>">Read more</a>
        </div>
        <% })%>
            <%include ./partials/footer.ejs%>